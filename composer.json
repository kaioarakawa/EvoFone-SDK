{
    "name": "kaioarakawa/evofone-sdk",
    "description": "A SDK for interacting with Evolution API",
    "type": "library",
    "keywords": ["laravel", "EvoFone"],
    "license": "MIT",

    "authors": [
        {
            "name": "Kaio Arakawa",
            "email": "kaioarakawa@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3.1|^7.0"
    },
    "require-dev": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.3.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.4|^9.0|^10.4"
    },
    "autoload": {
        "psr-4": {
            "EvoFone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "EvoFone\\FoneEvoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
